        "/v1/organisations/{organisation_id}/sites/{site_id}/meters/{meter_id}/readings": {
            "get": {
                "tags": ["Reading"],
                "description": "Get past meter readings up to 6 months old. This method uses cursor based pagination.",
                "summary": "Retrieve past meter readings",
                "parameters": [
                    {
                        "name": "organisation_id",
                        "in": "path",
                        "description": "UUID of the organisation ID",
                        "required": true,
                        "example": "9bb2dd2b-05bd-4e34-97fb-2249e687630b",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "site_id",
                        "in": "path",
                        "description": "UUID of the site ID",
                        "required": true,
                        "example": "291c809c-cbf6-407b-8a8f-e8fd1f819027",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "meter_id",
                        "in": "path",
                        "description": "UUID of the meter ID",
                        "required": true,
                        "example": "506a5c5f-0b1f-4cb1-9dc2-42a172066f0a",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "last_evaluated_key",
                        "in": "query",
                        "description": "Use this query parameter if the response body returns a last_evaluated_key value. If the last evaluated key is not empty, there are more results to retrieve.",
                        "required": false,
                        "example": "2023-09-19T09:03:42.415Z",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "items_per_page",
                        "in": "query",
                        "description": "Specify the number of items to return in each response.",
                        "required": false,
                        "example": 25,
                        "schema": { "type": "integer", "default": 50 }
                    },
                    {
                        "name": "start_datetime",
                        "in": "query",
                        "description": "Specify the starting date and time to return readings from.  If no end_datetime query is provided to the request, the response  will provide all available readings from the start date time up until 6 months old from the current datetime.  ",
                        "required": false,
                        "example": "2023-09-19T09:02:44.000Z",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "end_datetime",
                        "in": "query",
                        "description": "Specify the end date and time to return readings to.  If no start_datetime query is provided to the request,  the response will provide all available readings from  the end date time up until the current datetime.  ",
                        "required": false,
                        "example": "2023-09-19T09:02:44.000Z",
                        "schema": { "type": "string" }
                    },
                    {
                        "name": "interval",
                        "in": "query",
                        "description": "Specify the aggregation interval of the readings in the response. The aggregation is the average of all the values in a time window with a size being the interval  ",
                        "required": false,
                        "example": 30,
                        "schema": {
                            "type": "integer",
                            "default": 15,
                            "enum": [15, 30, 60]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/readingsResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/badRequest"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/unauthorised"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/notFound"
